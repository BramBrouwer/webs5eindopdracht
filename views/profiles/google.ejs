<div class="col-sm-6 col-sm-offset-3">
    <h1><span class="fa fa-google-plus" aria-hidden="true"></span> Profile</h1>
    <div style="padding-bottom: 10%">
        <div class="row">
            <div class="col-sm-9" style="border-right:2px solid black">
                <div class="col-xs-3">ID:</div> <div class="col-xs-6"><%= user._id %></div>
            </div>
            <div class="col-sm-3">
                <div class="col-sm-9"><a href="#" class="btn btn-block btn-xs btn-danger" style="border: 0px;">Delete</a></div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-9" style="border-right:2px solid black">
                <div class="col-xs-3">Name: </div> <div class="col-xs-6"><%= user.google.name %></div>
            </div>
            <div class="col-sm-3">
                <div class="col-sm-9"><a href="#" class="btn btn-block btn-xs btn-success" style="border: 0px;">Edit</a></div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-9" style="border-right:2px solid black">
                <div class="col-xs-3">E-Mail: </div> <div class="col-xs-6"><%= user.google.email %></div>
            </div>
            <div class="col-sm-3" >
                <div class="col-xs-9"><a href="#" class="btn btn-block btn-xs btn-success" style="border: 0px;">Edit</a></div>
            </div>
        </div>
    </div>
    <div class="panel-group" role="tablist">
        <div class="panel panel-default">
            <div class="panel-heading" role="tab" id="collapseListGroupHeading1">
                <h4 class="panel-title"> <a href="#collapseListGroup1" class="collapsed" role="button" data-toggle="collapse" aria-expanded="false" aria-controls="collapseListGroup1">Waypoints</a> </h4> </div>
            <div class="panel-collapse collapse" role="tabpanel" id="collapseListGroup1" aria-labelledby="collapseListGroupHeading1" aria-expanded="false">
                <ul class="list-group">
                    <% for(var i=0;i < user.waypoints.length;i++){ %>
                        <li class="list-group-item"><%= user.waypoints[i] %></li>
                        <% var exists = true; %>
                    <% } %>
                    <% if (!exists){ %>
                        <li class="list-group-item">No waypoints found</li>
                    <% } %>
                </ul>
            </div>
        </div>
    </div>
</div>